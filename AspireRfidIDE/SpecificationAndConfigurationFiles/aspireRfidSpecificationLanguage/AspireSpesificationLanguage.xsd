	targetNamespace="urn:ow2:aspirerfid:apdspec:xsd:1" xmlns:ale="urn:epcglobal:ale:xsd:1" xmlns:alelr="urn:epcglobal:alelr:xsd:1"
	xmlns:aspirepds="urn:ow2:aspirerfid:apdspec:xsd:1" xmlns:epcismd="urn:epcglobal:epcis-masterdata:xsd:1"
	xmlns:xpdl="http://www.wfmc.org/2002/XPDL1.0">

