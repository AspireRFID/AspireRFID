<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	<sql-type jdbc-type="INTEGER" hibernate-type="int"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="150"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
</type-mapping>
<table-filter match-catalog="epcis_tracking" match-name="epc_class_properties"/>
  <table-filter match-catalog="epcis_tracking" match-name="epcid_foreign_data"/>
  <table-filter match-catalog="epcis_tracking" match-name="remote_uri"/>
  <table-filter match-catalog="epcis_tracking" match-name="reported_epcid"/>
</hibernate-reverse-engineering>